    <children>
        <Button fx:id="button" layoutX="126" layoutY="90" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <ImageView fx:id="imgHeroe1" fitHeight="100.0" fitWidth="100.0" layoutX="0.0" layoutY="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Heroes/Arquera/Atacando1.gif" />
         </image>
      </ImageView>
      <ImageView fx:id="imgArma1" fitHeight="100.0" fitWidth="100.0" layoutX="0.0" layoutY="0.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Armas/Mazo/Atacando5.gif" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
